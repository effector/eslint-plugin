{
  "name": "eslint-plugin-effector",
  "version": "0.10.5",
  "description": "Enforcing best practices for Effector",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "effector"
  ],
  "repository": "effector/eslint-plugin",
  "main": "index.js",
  "author": "Igor Kamyshev <igor@kamyshev.me>",
  "license": "MIT",
  "packageManager": "pnpm@7.28.0",
  "scripts": {
    "test": "jest",
    "docs:prepare": "node ./docs/scripts/prepare-toc.js && node ./docs/scripts/prepare-presets.js && node ./docs/scripts/prepare-changelog.js",
    "docs:build": "pnpm docs:prepare && vitepress build docs",
    "docs:dev": "pnpm docs:prepare && vitepress dev docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^14 || ^15 || ^16 || ^17 || ^18 || ^19 || ^20"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@typescript-eslint/experimental-utils": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "clean-publish": "^3.2.0",
    "effector": "^22.0.0",
    "effector-react": "^22.0.6",
    "eslint": "^8.0.0",
    "glob": "^8.0.3",
    "jest": "^27.1.0",
    "nano-staged": "^0.5.0",
    "react": "^17.0.2",
    "simple-git-hooks": "^2.7.0",
    "typescript": "^4.4.2",
    "vitepress": "1.0.0-alpha.65",
    "vue": "^3.2.45"
  },
  "peerDependencies": {
    "effector": "*",
    "eslint": "7 || 8"
  },
  "dependencies": {
    "prettier": "^2.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*.{js,ts,md}": "prettier --write"
  }
}
