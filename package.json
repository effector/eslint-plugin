{
  "name": "eslint-plugin-effector",
  "version": "0.17.0",
  "description": "Enforcing best practices for Effector",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "effector"
  ],
  "homepage": "https://eslint.effector.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/effector/eslint-plugin"
  },
  "license": "MIT",
  "author": "Igor Kamyshev <igor@kamyshev.me>",
  "contributors": [
    {
      "name": "Mikhail Kireev",
      "url": "https://github.com/kireevmp"
    }
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "lint": "eslint",
    "prepack": "pnpm build",
    "test": "vitest run"
  },
  "dependencies": {
    "@typescript-eslint/type-utils": "^8.55.0",
    "@typescript-eslint/utils": "^8.55.0",
    "esquery": "^1.7.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^10.0.1",
    "@types/esquery": "^1.5.4",
    "@types/estree": "^1.0.8",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@typescript-eslint/rule-tester": "^8.55.0",
    "@vitest/coverage-v8": "^4.0.18",
    "effector": "^23.4.4",
    "effector-react": "^23.3.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "prettier-plugin-embed": "^0.5.1",
    "tsdown": "0.20.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitepress": "2.0.0-alpha.15",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0 || ^10.0.0",
    "typescript": ">=5.0.0"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc",
  "engines": {
    "node": "^20.9.0 || >=21.1.0"
  }
}
