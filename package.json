{
  "name": "eslint-plugin-effector",
  "version": "0.15.0",
  "description": "Enforcing best practices for Effector",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "effector"
  ],
  "repository": "effector/eslint-plugin",
  "main": "index.js",
  "author": "Igor Kamyshev <igor@kamyshev.me>",
  "license": "MIT",
  "packageManager": "pnpm@8.10.5",
  "scripts": {
    "test": "jest",
    "docs:prepare": "node ./docs/scripts/prepare-toc.js && node ./docs/scripts/prepare-presets.js && node ./docs/scripts/prepare-changelog.js",
    "docs:build": "pnpm docs:prepare && vitepress build docs",
    "docs:dev": "pnpm docs:prepare && vitepress dev docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^16 || ^17 || ^18 || ^19 || ^20 || ^21 || ^22 || ^23"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/rule-tester": "^6.0.0",
    "@typescript-eslint/utils": "^6.0.0",
    "clean-publish": "^3.2.0",
    "effector": "^23",
    "effector-react": "^23",
    "eslint": "^8.0.0",
    "glob": "^8.0.3",
    "jest": "^29.0.0",
    "nano-staged": "^0.5.0",
    "react": "^17.0.2",
    "simple-git-hooks": "^2.7.0",
    "typescript": "^5.0.0",
    "vitepress": "1.0.0-alpha.65",
    "vue": "^3.2.45"
  },
  "peerDependencies": {
    "effector": "^23",
    "eslint": "7 || 8 || 9"
  },
  "dependencies": {
    "prettier": "^2.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*.{js,ts,md}": "prettier --write"
  }
}
